{
  "defaultSeverity": "error",
  "extends": [
    "tslint-eslint-rules",
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "trailing-comma": false,
    "brace-style": [true, "stroustrup"],
    "one-line": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-var-requires": false
  },
  "rulesDirectory": []
}
